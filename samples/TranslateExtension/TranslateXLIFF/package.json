{
	"name": "translateextension",
	"displayName": "TranslateExtension",
	"description": "Translate XILFF files in a Visual Studio code workpace",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.translatexliff"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.translatexliff",
				"title": "Translate XLIFF files"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^8.10.25",
		"@types/vscode": "^1.32.0",
		"@types/xmldom": "^0.1.29",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.17.0",
		"typed-rest-client": "^1.4.0",
		"typescript": "^3.5.1",
		"vscode-test": "^1.0.0",
		"xmldom": "^0.1.27"
	},
	"dependencies": {}
}
